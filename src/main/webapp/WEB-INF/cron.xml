<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/task/updateTakeoffData</url>
        <description>Update data for takeoffs updated/added since last run</description>
        <schedule>every day 03:00</schedule>
        <target>4</target>
    </cron>
    <cron>
        <url>/task/cleanCache</url>
        <description>Delete cached data</description>
        <schedule>every day 05:00</schedule>
        <target>4</target>
    </cron>
</cronentries>